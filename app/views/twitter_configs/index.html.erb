<h1>Listing twitter_configs</h1>

<table>
  <tr>
  </tr>

<% for twitter_config in @twitter_configs %>
  <tr>
    <td><%= link_to 'Show', twitter_config %></td>
    <td><%= link_to 'Edit', edit_twitter_config_path(twitter_config) %></td>
    <td><%= link_to 'Destroy', twitter_config, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New twitter_config', new_twitter_config_path %>
